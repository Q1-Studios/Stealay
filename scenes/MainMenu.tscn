[sub_resource type="GDScript" id="GDScript_ow34m"]
resource_name = "buttonlogic"
script/source = "extends Control

func _ready() -> void:
	show()
	Globals.previous_sequence = [] 
	Globals.previous_move_count = 0

func showMe() -> void:
	show()


func HideMe() -> void:
	hide()
"

[sub_resource type="GDScript" id="GDScript_826wr"]
resource_name = "StartGame"
script/source = "extends SmartButton


func startGame() -> void:
	await get_tree().create_timer(1).timeout
	Globals.tutorial_enabled = true
	SceneManager.change_scene(SceneManager.planning_scene)
"

[sub_resource type="GDScript" id="GDScript_2wh52"]
resource_name = "quitButton"
script/source = "extends SmartButton

signal pre_quit_hook

func forcequit():
	await get_tree().create_timer(1).timeout
	pre_quit_hook.emit()
	get_tree().quit()
"

[sub_resource type="GDScript" id="GDScript_l72w3"]
resource_name = "buttonlogic2"
script/source = "extends Control

signal page_flip()

@export var first_button: Button
@export var triggered_by: Button

func showMe() -> void:
	page_flip.emit()
	show()
	first_button.grab_focus()


func HideMe() -> void:
	page_flip.emit()
	hide()
	triggered_by.grab_focus()
	
"

